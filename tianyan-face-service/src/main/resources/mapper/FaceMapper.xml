<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.newland.tianyan.face.dao.FaceMapper">

    <select id="selectIdByUserId" resultType="Long">
        SELECT id FROM faceDO a WHERE a.user_id = #{userId}
    </select>

    <select id="selectIdByGroupId" resultType="Long">
        SELECT id FROM faceDO a WHERE a.group_id = #{groupId}
    </select>

    <insert id="insertBatch">
        INSERT INTO faceDO ( id,app_id,gid,uid,group_id,user_id,image_path,features )
        VALUES
        <foreach collection="faceDOList" item="item" separator=",">
            ( #{item.id},#{item.appId},#{item.gid},#{item.uid},#{item.groupId},#{item.userId},#{item.imagePath},#{item.features} )
        </foreach>
    </insert>
</mapper>